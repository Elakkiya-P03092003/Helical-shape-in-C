#include <stdio.h>

int main()
{
    char str[50];
    scanf("%[^\n]s",str);
    int i,j,l=0;
    for(i=0;str[i]!='\0';i++)
    l++;
    for(i=0;i<str[i];i++){
        for(j=0;j<str[j];j++){
            if(i==j || i+j==(l-1)){
                printf("%c\n",str[j]);
                
            }
            else{
                printf(" ");
            }
        }
    }

    return 0;
}
